<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'AnalyticsApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'AnalyticsApplication'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'AnalyticsApplication'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'AnalyticsApplication'</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'AnalyticsApplication'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'AnalyticsApplication'</Description>
          <Count>24</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'AnalyticsApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'AnalyticsApplication'</Description>
          <Count>47</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Surround With...">
        <Item>
          <IdeaActionID>SurroundWith</IdeaActionID>
          <ShortCut>'Ctrl+Alt+T'</ShortCut>
          <Description>Surround With...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>